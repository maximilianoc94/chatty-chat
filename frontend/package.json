{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reach/router": "^1.3.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "node-sass": "^4.13.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-scripts": "3.3.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "component": "node scripts/component-creator.js"
  },
  "eslintConfig": {
    "plugins": [
      "jsx-a11y"
    ],
    "extends": [
      "react-app",
      "plugin:jsx-a11y/strict"
    ],
    "rules": {
      "jsx-a11y/label-has-for": 0,
      "getter-return": 2,
      "no-async-promise-executor": 2,
      "no-await-in-loop": 2,
      "no-cond-assign": 2,
      "no-console": 2,
      "no-constant-condition": 2,
      "no-debugger": 2,
      "no-dupe-keys": 2,
      "no-duplicate-case": 2,
      "no-prototype-builtins": 2,
      "no-obj-calls": 2,
      "no-empty": 2,
      "no-func-assign": 2,
      "no-unreachable": 2,
      "use-isnan": 2,
      "no-alert": 2,
      "no-empty-function": 2,
      "no-implicit-coercion": 2,
      "no-redeclare": 2,
      "no-return-assign": 2,
      "no-self-assign": 2,
      "no-self-compare": 2,
      "no-unused-expressions": 2,
      "no-global-assign": 2,
      "require-await": 2,
      "no-shadow": 2,
      "no-shadow-restricted-names": 2,
      "no-unused-vars": 2,
      "no-use-before-define": 2,
      "arrow-parens": 2,
      "arrow-spacing": 2,
      "no-const-assign": 2,
      "no-duplicate-imports": 2,
      "no-var": 2,
      "prefer-const": 2,
      "prefer-template": 2,
      "no-mixed-spaces-and-tabs": 2,
      "no-multiple-empty-lines": 2,
      "require-yield": 2
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-plugin-jsx-a11y": "^6.2.3"
  }
}
